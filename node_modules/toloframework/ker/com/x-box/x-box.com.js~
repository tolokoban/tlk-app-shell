/**
 * Component x-box
 */

exports.tags = ["x-box"];
exports.priority = 0;

/**
 * Compile a node of the HTML tree.
 */
exports.compile = function(root, libs) {
    var att = root.attribs;
    var width = att.width || att.w || 240;
    var height = att.height || att.h || 240;

    var box = {type: libs.Tree.TAG, name: "div", }
};
